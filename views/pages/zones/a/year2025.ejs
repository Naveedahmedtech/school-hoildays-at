<div class="main">
  <main id="main">
    <div id="menu_annee">
      <ul>
        <li>
          <a href="/zones/a-year-2024" title="vacances scolaires 2025 zone A"
            >2024-2025</a
          >
        </li>
        <li>
          <a
            href="/zones/a-year-2025"
            id="actif"
            title="vacances scolaires 2026 zone A"
            >2025-2026</a
          >
        </li>
      </ul>
    </div>
    <h1>Vacances scolaires zone A 2025-2026</h1>
    <% vacations.forEach(vacation => { %>
    <div class="dateTitre">
      <a href="<%= vacation.link %>"><%= vacation.title %></a>
    </div>
    <div class="date dA">
      <div class="zone zA"><span><%= vacation.zoneA %></span></div>
      <span class="lesdates">
        <% if (vacation.startDate) { %> Fin des cours :
        <b><%= vacation.startDate %></b><br />
        <% } %> <% if (vacation.endDate) { %> Jour de reprise :
        <b><%= vacation.endDate %></b>
        <% } %>
      </span>
    </div>
    <% }); %>
    <div class="spacer"></div>
    <div class="dateTitre">
      <span>Les académies rattachées à la zone A :</span>
    </div>
    <div class="date dA">
      <div class="zone zA"><span>Zone A</span></div>
      <span class="lesdates"
        ><a href="/academie-besancon/" class="noir">Besançon</a>,
        <a href="/academie-bordeaux/" class="noir">Bordeaux</a>,
        <a href="/academie-clermont-ferrand/" class="noir">Clermont-Ferrand</a>,
        <a href="/academie-dijon/" class="noir">Dijon</a>,
        <a href="/academie-grenoble/" class="noir">Grenoble</a>,
        <a href="/academie-limoges/" class="noir">Limoges</a>,
        <a href="/academie-lyon/" class="noir">Lyon</a>,
        <a href="/academie-poitiers/" class="noir">Poitiers</a></span
      >
    </div>
  </main>
</div>
